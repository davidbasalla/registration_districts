<%= render "scottish_mappings_header" %>

<table class="table">
<thead>
  <tr>
    <th>#</th>
    <th>Office Name</th>
    <th>Local Authority</th>
    <th>Registration District Number</th>
    <th>Type</th>
  </tr>
</thead>

<% @mappings.each_with_index do |mapping, index| %>
<tr>
  <td><%= index + 1 %></td>
  <td><%= mapping.name %></td>
  <td><%= mapping.council %></td>
  <td><%= mapping.reg_id %></td>
  <td><%= mapping.type %></td>
</tr>
<% end %>
</table>
