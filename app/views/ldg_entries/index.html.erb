<%= render "ldg_header" %>

<table class="table">
<thead>
  <tr>
    <th>#</th>
    <th>Title</th>
    <th>Town/City</th>
    <th>Postcode</th>
    <th>URL</th>
  </tr>
</thead>

<% @entries.each_with_index do |entry, index| %>
<tr>
  <td><%= index + 1 %></td>
  <td><%= entry.title %></td>
  <td><%= entry.town_city %></td>
  <td><%= entry.postcode %></td>
  <td><%= entry.url %></td>
</tr>
<% end %>
</table>
